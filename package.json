{
	"name": "le-bot-en-js",
	"version": "3.8.0",
	"description": "Le bot discord en JS.",
	"main": "src/index.js",
	"scripts": {
		"start": "node .",
		"test": "eslint src && prettier --check src/**/*.js",
		"docker-start": "docker-compose -f ./docker/docker-compose.yml up -d",
		"docker-stop": "docker-compose -f ./docker/docker-compose.yml down -d",
		"docker-compose-build-start": "docker-compose -f ./docker/docker-compose-build.yml up -d --build",
		"docker-compose-build-stop": "docker-compose -f ./docker/docker-compose-build.yml down -d"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TanguyChiffoleau/Le-bot-en-JS.git"
	},
	"author": "Tanguy Chiffoleau",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/TanguyChiffoleau/Le-bot-en-JS/issues"
	},
	"homepage": "https://github.com/TanguyChiffoleau/Le-bot-en-JS#readme",
	"dependencies": {
		"bent": "^7.3.12",
		"discord.js": "^13.6.0"
	},
	"devDependencies": {
		"dotenv": "^14.1.0",
		"eslint": "^8.6.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"prettier": "^2.5.1"
	},
	"type": "module"
}
